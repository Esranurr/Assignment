      "Enter a sentence : hippo runs to us!\n",
      "{'h': 1, 'i': 1, 'p': 2, 'o': 2, ' ': 3, 'r': 1, 'u': 2, 'n': 1, 's': 2, 't': 1, '!': 1}\n"
     ]
    }
   ],
   "source": [
    "sentence = input(\"Enter a sentence : \")\n",
    "\n",
    "list1 = []\n",
    "list2 = []\n",
    "\n",
    "for i in sentence :\n",
    "  if not(i in list1) :\n",
    "    list1.append(i)\n",
    "\n",
    "for i in list1 :\n",
    "  counter = 0\n",
    "\n",
    "  for j in sentence :\n",
    "    if j == i :\n",
    "      counter +=1\n",
    "  list2.append(counter)\n",
    "\n",
    "\n",
    "print(dict(zip(list1, list2)))"
   ]
