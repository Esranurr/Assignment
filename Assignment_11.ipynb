     "text": [
      "Enter a year please: 2022\n",
      "2022 is not a leap year\n"
     ]
    }
   ],
   "source": [
    "year = int(input(\"Enter a year please: \"))\n",
    "\n",
    "leap = (year % 4 == 0) and bool((year % 100) or not (year % 400))\n",
    "\n",
    "if leap :\n",
    "  print(\"{} is a leap year\".format(year))\n",
    "else :\n",
    "  print(\"{} is not a leap year\".format(year))"
   ]
